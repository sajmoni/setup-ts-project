{
  "name": "setup-ts-project",
  "version": "0.0.3",
  "description": "Setup everything you need for a TypeScript project",
  "keywords": [
    "TypeScript"
  ],
  "license": "MIT",
  "author": "sajmoni",
  "type": "module",
  "bin": "dist/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "go": "npm run build && preview && rm -rf example && mkdir example && cd example && setup-ts-project",
    "go-test": "npm run build && preview && npm test",
    "release": "npm run build && np --no-tests",
    "test": "vitest"
  },
  "prettier": "prettier-config-one",
  "dependencies": {
    "chalk": "5.2.0",
    "execa": "7.1.1",
    "tasuku": "2.0.1"
  },
  "devDependencies": {
    "np": "8.0.2",
    "package-preview": "4.0.0",
    "prettier": "2.8.8",
    "prettier-config-one": "0.1.1",
    "tempy": "3.0.0",
    "tsconfig-one": "0.0.4",
    "typescript": "5.1.3",
    "vitest": "0.31.4"
  },
  "engines": {
    "node": ">=18"
  }
}
