{
  "name": "setup-ts-project",
  "version": "0.2.0",
  "description": "Setup everything you need for a TypeScript project",
  "keywords": [
    "TypeScript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sajmoni/setup-ts-project.git"
  },
  "license": "MIT",
  "author": "sajmoni",
  "type": "module",
  "bin": "dist/src/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "npm run build && chmod +x dist/src/index.js && vitest",
    "release": "npm run build && np --no-tests"
  },
  "prettier": "prettier-config-one",
  "dependencies": {
    "cac": "6.7.14",
    "chalk": "5.4.1",
    "execa": "9.5.2",
    "read-pkg": "9.0.1",
    "sort-package-json": "2.14.0",
    "tasuku": "2.0.1",
    "write-pkg": "7.0.0",
    "write-pretty-file": "0.2.0"
  },
  "devDependencies": {
    "@types/node": "20.17.19",
    "get-bin-path": "11.0.0",
    "np": "10.2.0",
    "prettier": "3.5.1",
    "prettier-config-one": "0.2.1",
    "tempy": "3.1.0",
    "tsconfig-one": "0.0.10",
    "typescript": "5.7.3",
    "vitest": "3.0.5"
  },
  "engines": {
    "node": ">=22"
  }
}
